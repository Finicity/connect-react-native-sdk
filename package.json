{
  "name": "@finicity/connect-react-native",
  "version": "0.0.2-alpha.5",
  "description": "Finicity Connect SDK for ReactNative",
  "main": "dist/commonjs/index.js",
  "module": "dist/module/index.js",
  "types": "dist/typescript/index.d.ts",
  "keywords": [
    "finicity",
    "finicity connect"
  ],
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build": "bob build",
    "prettier-check": "prettier --check .",
    "test:version_bump": "test $npm_package_version != $(npm show $npm_package_name version) || (echo 'Failing... You must bump package.json and package-lock.json' && exit 1)",
    "files": [
      "dist/",
      "src/"
    ],
    "husky": {
      "hooks": {
        "pre-push": "ng lint && prettier --check ."
      }
    }
  },
  "dependencies": {
    "husky": "^5.1.3",
    "install": "^0.13.0",
    "prettier": "^2.2.1",
    "react": "16.13.1",
    "react-native": "^0.63.2",
    "react-native-inappbrowser-reborn": "^3.5.1",
    "react-native-webview": "^11.2.4"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@react-native-community/bob": "^0.17.1",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "npm-version": "^1.1.0",
    "typescript": "~4.0.0"
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "module",
      "typescript",
      "commonjs"
    ]
  }
}
