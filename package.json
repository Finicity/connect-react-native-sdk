{
  "name": "@finicity/connect-react-native",
  "version": "0.0.1-alpha.3",
  "description": "Finicity Connect SDK for ReactNative using Expo",
  "main": "dist/module/index.js",
  "keywords": [
    "finicity",
    "finicity connect"
  ],
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build": "bob build",
    "prettier-check": "prettier --check .",
    "test:version_bump": "test $npm_package_version != $(npm show $npm_package_name version) || (echo 'Failing... You must bump package.json and package-lock.json' && exit 1)",
    "main": "dist/module/index.js",
    "module": "dist/module/index.js",
    "react-native": "src/index.ts",
    "types": "dist/typescript/src/index.d.ts",
    "files": [
      "dist/",
      "src/"
    ],
    "husky": {
      "hooks": {
        "pre-push": "ng lint && prettier --check ."
      }
    }
  },
  "dependencies": {
    "expo": "~40.0.0",
    "expo-status-bar": "~1.0.3",
    "expo-web-browser": "~8.6.0",
    "husky": "^5.1.3",
    "prettier": "^2.2.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "^0.63.2",
    "react-native-web": "~0.13.12",
    "react-native-webview": "^11.2.4"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@react-native-community/bob": "^0.17.1",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "npm-version": "^1.1.0",
    "typescript": "~4.0.0"
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "module",
      "typescript"
    ]
  }
}
